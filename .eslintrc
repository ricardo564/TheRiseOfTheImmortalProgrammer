{
  "extends": ["@antfu", "plugin:astro/recommended"
],
  "rules": {
    "vue/max-attributes-per-line": ["error", {
      "singleline": {
        "max": 1
      },
      "multiline": {
        "max": 1
      },
    }],
    "vue/component-tags-order": [
      "error",
      {
        "order": [
          [
            "script",
            "template"
          ],
          "style"
        ]
      }
    ],
    "vue/order-in-components": [
      "error",
      {
        "order": [
          "el",
          "name",
          "key",
          "parent",
          "functional",
          "layout",
          [
            "delimiters",
            "comments"
          ],
          [
            "directives",
            "filters"
          ],
          "extends",
          "mixins",
          [
            "provide",
            "inject"
          ],
          "ROUTER_GUARDS",
          "components",
          "middleware",
          "validate",
          "scrollToTop",
          "transition",
          "loading",
          "inheritAttrs",
          "model",
          "emits",
          "LIFECYCLE_HOOKS",
          [
            "props",
            "propsData"
          ],
          "setup",
          "asyncData",
          "data",
          "fetch",
          "head",
          "computed",
          "watch",
          "watchQuery",
          "methods",
          [
            "template",
            "render"
          ],
          "renderError"
        ]
      }
    ],
    "vue/attributes-order": [
      "error",
      {
        "order": [
          "DEFINITION",
          "LIST_RENDERING",
          "CONDITIONALS",
          "TWO_WAY_BINDING",
          "CONTENT",
          "RENDER_MODIFIERS",
          [
            "GLOBAL",
            "UNIQUE",
            "SLOT",
            "OTHER_ATTR"
          ],
          "OTHER_DIRECTIVES",
          "EVENTS"
        ],
        "alphabetical": false
      }
    ],
    "vue/no-reserved-component-names": 0,
    "vue/no-mutating-props": 1,
    "curly": 0,
    "no-console": "warn"
  },
  "overrides": [{
    "files": ["*.astro"],
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "extraFileExtensions": [".astro"]
    },
    "rules": {
      "astro/no-unused-css-selector": "error",
      "astro/prefer-class-list-directive": "error",
      "astro/semi": [
        "error",
        "never"
      ]
    }
  }]
}
